// #### HEADER ####
include side-nav/multi-mobile.pug
header.app-bar.header#header
  .container
    #menu_blank &nbsp;
    .header-content
      nav.nav-menu
        |
        |
        button.btn-icon.waves-effect.sidenav-trigger.show-md-down(
          id="mobile_menu"
          type="button"
          data-target="slide_menu"
        )
          i.material-icons menu
        .logo
          |
          |
          a(href="#home")
            img(
              src=assets + brand.starter.img
              alt="logo"
            )
          |
          |
        .scrollactive-nav.show-lg-up
          include top-nav/multi-level-click
      nav.user-menu
        |
        |
        button.btn.btn-flat.waves-effect.show-lg-up(type="button") Login
        |
        |
        button.btn.waves-effect.primary(type="button") Register
        |
        |
        span.spacer.vertical-divider.show-lg-up
        include ./settings
        +settings("false")
|
|
// #### END HEADER ####