// #### HEADER ####
header.app-bar.header#header
  .container
    .header-content
      nav.nav-menu
        |
        |
        button.btn-icon.waves-effect.sidenav-trigger.show-md-down(
          id="mobile_menu"
          type="button"
        )
          i.material-icons menu
        .logo
          |
          |
          a(href="#home")
            img(
              src=assets + brand.starter.img
              alt="logo"
            )
          |
          |
        .scrollactive-nav.scrollnav.show-lg-up
          ul
            each item in menu.starter
              |
              |
              li
                a.btn.btn-flat.anchor-link.waves-effect(
                  href='#'+item.name
                ) #{item.name}
      include ./top-nav/user-menu
// #### END HEADER ####